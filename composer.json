{
    "name": "aliw1382/telegram-tools",
    "description": "Package for tools building your telegram bot",
    "keywords": [
        "laravel",
        "telegram",
        "telegram bot",
        "laravel telegram bot",
        "laravel telegram sdk",
        "telegram api laravel"
    ],
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aliw1382\\TelegramTools\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ali Shahmohammadi",
            "email": "aliw1382@gmail.com",
            "homepage": "https://github.com/aliw1382/telegram-tools"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Aliw1382\\TelegramTools\\Tests\\": "src/Tests/"
        }
    }
}
